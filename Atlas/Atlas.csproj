<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <ApplicationIcon />
    <Win32Resource />
  </PropertyGroup>

  <ItemGroup>
    <None Remove="AtlasDB.db" />
    <None Remove="background.jpg" />
    <None Remove="box_inventory.png" />
    <None Remove="delivery.png" />
    <None Remove="home.png" />
    <None Remove="icons8-menu-24.png" />
    <None Remove="images\logoimage.png" />
    <None Remove="images\menu_button_default.png" />
    <None Remove="images\menu_button_mouseover.png" />
    <None Remove="inventory.png" />
    <None Remove="logoimage.png" />
    <None Remove="menu_button_default.png" />
    <None Remove="menu_button_mouseover.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="AtlasDB.db">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Dapper" Version="2.0.123" />
    <PackageReference Include="MaterialDesignThemes" Version="4.3.0" />
    <PackageReference Include="Microsoft.Data.Sqlite" Version="5.0.13" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="5.0.13" />
    <PackageReference Include="MySql.Data" Version="8.0.27" />
    <PackageReference Include="System.Data.SqlClient" Version="4.8.3" />
    <PackageReference Include="System.Data.SQLite.Core" Version="1.0.115.5" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="background.jpg" />
    <Resource Include="box_inventory.png" />
    <Resource Include="delivery.png" />
    <Resource Include="home.png" />
    <Resource Include="icons8-menu-24.png" />
    <Resource Include="images\logoimage.png" />
    <Resource Include="images\menu_button_mouseover.png" />
    <Resource Include="images\menu_button_default.png" />
    <Resource Include="inventory.png" />
    <Resource Include="logoimage.png" />
    <Resource Include="menu_button_default.png" />
    <Resource Include="menu_button_mouseover.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
  </ItemGroup>

</Project>
